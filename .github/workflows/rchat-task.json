{

  "family": "rchat-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::389364614461:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "rchat-container",
      "image": "${REPOSITORY_URI}:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
      
    }
  ]
}